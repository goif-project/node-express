<!DOCTYPE html>
<html>
  <head>
    <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <title><%= title %></title>
    <script src="/js/jquery.js"></script>
    <link rel='stylesheet' href='/css/style.css' />
  </head>
  <body>
    <div class="wrapper_rank">
      <h1><img src="../img/T6.png" alt="HOME GOLF"></h1>
      <p class="rank_btn"><a href="../">TOPへ戻る</a></p>
      <ul class="rank-list">
        <% Rank.forEach(function(scoreRank) { %>
          <li class="main-list__item">
            <p class="board__title"><%= scoreRank.user_name %></p>
            <p class="board__date"><%= scoreRank.score %>点</p>
            </a>
          </li>
        <% }); %>
      </ul>
      <p class="rank_btn"><a href="../">TOPへ戻る</a></p>
    </div>
  </body>
</html>
