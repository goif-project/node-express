<!DOCTYPE html>
<html>
  <head>
    <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="wrapper">
      <form action="http://localhost:3000" method="post" class="board-form">
        スコア: <input type="text" name="score" id="score" class="input" required>
        名前: <input type="text" name="user_name" id="user_name" class="input" required><br>
        <br>
        <button type="submit" id="score_post" class="submit">ボード作成</button>
      </form>
      <ul class="main-list">全体
        <% List.forEach(function(scoreList) { %>
          <li class="main-list__item">
            <p class="board__date"><%= scoreList.score %></p>
          </li>
        <% }); %>
      </ul>
    </div>
  </body>
</html>
